--Target Tab
local Section = TargetTab:AddSection({
    Name = "Target"
    })

local Players = game:GetService("Players")
local CurrentPlayer

local function getAllPlayers()
    local playerList = {}
    for _, player in ipairs(Players:GetPlayers()) do
        table.insert(playerList, player.Name)
        table.insert(playerList, player.DisplayName)
    end
    return playerList
end

local function teleportToPlayer(player)
    if player and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
        local humanoidRootPart = Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
        if humanoidRootPart then
            humanoidRootPart.CFrame = player.Character.HumanoidRootPart.CFrame
        end
    end
end

local function viewPlayer(player)
    local camera = workspace.CurrentCamera
    camera.CameraSubject = player.Character
    camera.CameraType = Enum.CameraType.Follow
end

local function resetCamera()
    local camera = workspace.CurrentCamera
    camera.CameraSubject = Players.LocalPlayer.Character
    camera.CameraType = Enum.CameraType.Custom
end

TargetTab:AddTextbox({
    Name = "Player Here",
    Default = "",
    TextDisappear = true,
    Callback = function(Value)
        local allPlayers = Players:GetPlayers()
        for _, player in ipairs(allPlayers) do
            if string.find(string.lower(player.Name), string.lower(Value)) or string.find(string.lower(player.DisplayName), string.lower(Value)) then
                CurrentPlayer = player
                print("Player found: " .. player.Name)
                break
            end
        end
    end
})

TargetTab:AddToggle({
    Name = "View",
    Default = false,
    Callback = function(Value)
        if Value and CurrentPlayer then
            viewPlayer(CurrentPlayer)
            print("Now viewing: " .. CurrentPlayer.Name)
        else
            resetCamera()
            print("Camera reset to local player.")
        end
    end    
})

TargetTab:AddButton({
    Name = "Goto",
    Callback = function()
        if CurrentPlayer then
            teleportToPlayer(CurrentPlayer)
            print("Teleported to: " .. CurrentPlayer.Name)
        else
            print("No player selected to teleport to.")
        end
    end    
})

--Combat Tab
local Section = CombatTab:AddSection({
    Name = "Combat"
})

local player = game.Players.LocalPlayer or game.Players:GetPlayerFromCharacter(script.Parent)
local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")

local deepVoidPosition = Vector3.new(0, -10000, 0)
local toggleEnabled = false

local function forceReset()
    humanoid.Health = 0
end

local function teleportToDeepVoid()
    local rootPart = character:WaitForChild("HumanoidRootPart")
    rootPart.CFrame = CFrame.new(deepVoidPosition)
    forceReset()
end

local function isHealthBarRedOrOrange(health)
    local maxHealth = humanoid.MaxHealth
    local healthPercentage = health / maxHealth

    if healthPercentage <= 0.1 then
        return true
    elseif healthPercentage <= 0.2 then
        return true
    else
        return false
    end
end

local healthCheckConnection
local function enableHealthCheck()
    healthCheckConnection = humanoid.HealthChanged:Connect(function(health)
        if toggleEnabled and (health < 5 or isHealthBarRedOrOrange(health)) then
            teleportToDeepVoid()
        end
    end)
end

local function disableHealthCheck()
    if healthCheckConnection then
        healthCheckConnection:Disconnect()
    end
end

CombatTab:AddToggle({
    Name = "Anti-Stomp (Buggy)",
    Default = false,
    Callback = function(Value)
        toggleEnabled = Value
        if toggleEnabled then
            enableHealthCheck()
        else
            disableHealthCheck()
        end
    end    
})

CombatTab:AddToggle({
    Name = "Auto Reload",
    Default = false,
    Callback = function(Value)
        _G.AutoReload = Value

        if _G.AutoReload then
            while _G.AutoReload and game:GetService("RunService").Heartbeat:Wait() do
                if game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool") then
                    local tool = game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool")
                    local ammo = tool:FindFirstChild("Ammo")

                    if ammo and ammo.Value <= 0 then
                        game:GetService("ReplicatedStorage").MainEvent:FireServer("Reload", tool) 
                        wait(1)
                    end
                end
            end
        end
    end    
})

